BEGIN
    n = 6
    CALL fib
END

DEF fib
    IFZ n
        result = 1
        RETURN
    END
    IFZ n - 1
        result = 1
        RETURN
    END

    result = 0
    PUSH n END
    n = n - 1
    CALL fib
    POP n END

    PUSH result END
    n = n - 2
    CALL fib
    temp = result
    POP result END
    result = result + temp
END